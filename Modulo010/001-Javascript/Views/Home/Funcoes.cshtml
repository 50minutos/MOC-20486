@section script{
    <script type="text/javascript">
        var p = {};
        //var p = new Object();

        p.codigo = 1;
        p.nome = "adão";
        p.sexo = "M";

        document.writeln("<p>" + p.codigo + "</p>");
        document.writeln("<p>" + p.nome + "</p>");
        document.writeln("<p>" + p.sexo + "</p>");
        document.writeln("<p>" + JSON.stringify(p) + "</p>");
        document.writeln("<p>" + p.toString() + "</p>");

        p.acordar = function () {
            document.writeln("<p>*** " + this.nome + " acordou ***</p>");
        }

        p.acordar();
    </script>
}